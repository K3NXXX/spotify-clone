{
  "expo": {
    "name": "spotify-clone",
    "slug": "spotify-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.spotifyclone",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    
    },
    "plugins": [
      [
        "expo-font"
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "2a8e72c4-2c0a-485f-a172-7c8067f3e3c8"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2a8e72c4-2c0a-485f-a172-7c8067f3e3c8"
    }
  }
}
